{
    "$id": "https://juezlti.eu/YAPEXIL_RESOURCE",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "PERL Request Schema",
    "description": "A schema for PEARL REPLY ",
    "type": "object",
    "properties": {
        "pathname": {
            "title": "Pathname",
            "description": "Path to file in the archive.",
            "type": "string"
        },
        "type": {
            "title": "Type",
            "description": "Type of resource.",
            "enum": [
                "EMBEDDABLE",
                "LIBRARY"
            ],
            "type": "string"
        }
    },
    "required": [
        "pathname",
        "type"
    ]
}